oters.json\",\n        {\n          method: \"POST\",\n          body: JSON.stringify(voterData),\n          headers: {\n            \"content-type\": \"application.json\",\n          },\n        }\n      );\n\n      if (!response.ok) {\n        throw new Error(\"Something is Wrong \");\n      }\n\n      const data = await response.json();\n      console.log(data);\n    } catch (error) {\n      setError(error.message);\n    }\n  };\n\n  return (\n    <>\n      <Navbar />\n      <div className={styles.electionPageCover}>\n        <Sidebar />\n        <div className={styles.pageContent}>\n          <h2>Add Voter</h2>\n\n          <div className={styles.imageCover}>\n            <img src=\"\\images\\add-voter-image.png\" alt=\"add-voter-page-image\" />\n          </div>\n          \n\n          <form onSubmit={onSubmitHandler}>\n            <div\n              className={`${styles[\"addVoterForm\"]} ${\n                nameIsInvalid || idIsInvalid ? styles.invalid : \"\"\n              }`}\n            >\n              <p>Please Enter Voter's Details</p>\n              <label htmlFor=\"voter-name\">Name</label>\n              <input\n                id=\"voter-name\"\n                type=\"text\"\n                placeholder=\"Enter Voter's Name\"\n                onChange={nameInputHandler}\n                onBlur={nameBlurHandler}\n                value={enteredName}\n              />\n\n              {nameIsInvalid ? (\n                <p className={styles.errorMessage}>\n                  Please Enter a Valid Voter Name\n                </p>\n              ) : (\n                \"\"\n              )}\n\n              <label htmlFor=\"voter-id\">Voter Id</label>\n              <input\n                id=\"voter-id\"\n                type=\"text\"\n                placeholder=\"Enter Voter's Id\"\n                onChange={idInputHandler}\n                onBlur={idBlurHandler}\n                value={enteredId}\n              />\n\n              {idIsInvalid ? (\n                <p className={styles.errorMessage}>\n                  Please Enter a Valid Voter Id\n                </p>\n              ) : (\n                \"\"\n              )}\n\n              <div className={styles.formActions}>\n                <button className={styles.addVoterButton}>Add Voter</button>\n              </div>\n            </div>\n          </form>\n        </div>\n      </div>\n    </>\n  );\n};\n\nexport default AddVoter;\n","/Users/lasagna/DecentralVote/DecentralVote/Client/src/components/ElectionForm/ElectionForm.jsx",["134","135","136","137","138","139","140","141","142","143","144","145"],[],"/Users/lasagna/DecentralVote/DecentralVote/Client/src/components/AddCandidate/AddCandidate.jsx",["146","147","148","149","150"],[],"import styles from \"./AddCandidate.module.css\";\nimport Sidebar from \"../../ui/Sidebar/Sidebar\";\nimport Navbar from \"../../ui/Navbar/Navbar\";\nimport useInput from \"../../hooks/use-input\";\nimport { useState } from \"react\";\n\nconst AddCandidate = () => {\n  const [error, setError] = useState(null);\n\n  const {\n    value: enteredName,\n    valueIsValid: nameIsValid,\n    valueIsInvalid: nameIsInvalid,\n    valueBlurHandler: nameBlurHandler,\n    valueInputHandler: nameInputHandler,\n    reset: nameReset,\n  } = useInput((value) => {\n    return value && value.trim() != \"\";\n  });\n\n  const {\n    value: enteredId,\n    valueIsValid: idIsValid,\n    valueIsInvalid: idIsInvalid,\n    valueBlurHandler: idBlurHandler,\n    valueInputHandler: idInputHandler,\n    reset: idReset,\n  } = useInput((value) => {\n    return value && value.length >= 5;\n  });\n\n  const onSubmitHandler = (event) => {\n    const newCandidate = {\n      candidateName: enteredName,\n      candidateId: enteredId,\n    };\n\n    event.preventDefault();\n\n    onAddCandidate(newCandidate);\n\n    nameReset();\n    idReset();\n  };\n\n  const onAddCandidate = async (candidateData) => {\n    setError(null);\n\n    try {\n      const response = await fetch(\n        \"https://decentral-51b5a-default-rtdb.firebaseio.com/candidate.json\",\n        {\n          method: \"POST\",\n          body: JSON.stringify(candidateData),\n          headers: {\n            \"content-type\": \"application.json\",\n          },\n        }\n      );\n\n      if (!response.ok) {\n        throw new Error(\"Something is Wrong \");\n      }\n\n      const data = await response.json();\n      console.log(data);\n    } catch (error) {\n      setError(error.message);\n    }\n  };\n\n  return (\n    <>\n      <Navbar />\n      <div className={styles.candidatePageCover}>\n        <Sidebar />\n        <div className={styles.pageContent}>\n          <h2>Add Candidate</h2>\n\n          <div className={styles.imageCover}>\n            <img\n              src=\"\\images\\add-candidate-image.png\"\n              alt=\"add-voter-page-image\"\n            />\n          </div>\n\n          <form onSubmit={onSubmitHandler}>\n            <div\n              className={`${styles[\"addCandidateForm\"]} ${\n                nameIsInvalid || idIsInvalid ? styles.invalid : \"\"\n              }`}\n            >\n              <p>Please Enter Candidate's Details</p>\n              <label htmlFor=\"candidate-name\">Name</label>\n              <input\n                id=\"candidate-name\"\n                type=\"text\"\n                placeholder=\"Enter Candidate's Name\"\n                onChange={nameInputHandler}\n                onBlur={nameBlurHandler}\n                value={enteredName}\n              />\n\n              {nameIsInvalid ? (\n                <p className={styles.errorMessage}>\n                  Please Enter a Valid Candidate Name\n                </p>\n              ) : (\n                \"\"\n              )}\n\n              <label htmlFor=\"candidate-id\">Candidate Id</label>\n              <input\n                id=\"candidate-id\"\n                type=\"text\"\n                placeholder=\"Enter Candidate's Id\"